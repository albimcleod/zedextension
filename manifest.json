{
  "name": "StackedNaks - ZedRun",
  "action": {},
  "manifest_version": 3,
  "version": "1.0",
  "description": "Manage your racing stable from within ZedRun to improve your race results",
  "permissions": [
      "debugger"
    ],
  "content_scripts": [
   {
     "matches": ["https://zed.run/*", "https://www.stackednaks.com/*", "http://localhost:3000/race/*", "https://www.hawku.com/*", "https://knowyourhorses.com/*"],
     "css": ["styles.css"],
     "js": ["content.js"]
   }
 ],
  "icons": { 
     "16": "logo_16.png",
     "48": "logo_48.png",
     "128": "logo_128.png" 
  }
}